  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "decs.d.ts", "decs.d.ts"],
  "compilerOptions": {
    "baseUrl": ".",
